<h2>Users</h2>

<h2>User list</h2>
<table id="accountList">
	<tr>
		<th>Nick</th>
		<th>Name</th>
		<th>Age</th>
	</tr>
  {{#each model as |userdata|}}
  <tr>
    <td class="tree-item-capture">{{userdata.nickname}}</td>
    <td>{{userdata.name}}</td>
    <td>{{userdata.age}}</td>
  </tr>
  {{/each}}
</table>
